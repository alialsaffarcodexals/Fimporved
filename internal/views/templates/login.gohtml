{{define "body"}}
<div class="card">
  <h1>Login</h1>
  <form method="POST" action="/login">
    <label>Email</label>
    <input name="email" type="email" required>
    <label>Password</label>
    <input name="password" type="password" required>
    <div style="margin-top:.75rem;"><button class="btn btn-primary">Login</button></div>
  </form>
</div>
{{end}}
